<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:prism="http://prismlibrary.com"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:extensions="clr-namespace:WorkTimeSheet.Extensions"
            xmlns:views="clr-namespace:WorkTimeSheet.Views"
            prism:ViewModelLocator.AutowireViewModel="True"
            x:Class="WorkTimeSheet.Views.TabbedPageCollection">
    <views:Dashboard IconImageSource="{extensions:ImageResourceExtension WorkTimeSheet.Assets.dashboard.png}"/>
    <views:Logs IconImageSource="{extensions:ImageResourceExtension WorkTimeSheet.Assets.logs.png}"/>
    <views:Logout IconImageSource="{extensions:ImageResourceExtension WorkTimeSheet.Assets.logout.png}"/>
</TabbedPage>